{
  "expo": {
    "name": "Castafiore",
    "slug": "Castafiore",
    "description": "Mobile app for navidrome",
    "version": "2025.05.23",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "bundleIdentifier": "com.sawyerf.castafiore"
    },
    "android": {
      "package": "com.sawyerf.castafiore",
      "adaptiveIcon": {
        "foregroundImage": "./assets/foreground-icon.png",
        "backgroundColor": "#660000"
      },
      "splash": {
        "image": "./assets/foreground-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#660000"
      }
    },
    "web": {
      "favicon": "./assets/icon.png",
      "shortName": "Castafiore",
      "startUrl": "./index.html",
      "backgroundColor": "#121212",
      "theme_color": "#121212"
    },
    "extra": {
      "eas": {
        "projectId": "98d27f72-714e-415c-99f9-30f3f78d68e2"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}
